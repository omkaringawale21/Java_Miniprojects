package org.omkaringawale.dithree;

public class Student {

	private int sno;
	private String sname;
	private int age;
	private Address address;
	
	public Student(int sno, String sname, int age) {
		super();
		this.sno = sno;
		this.sname = sname;
		this.age = age;
	}
	
	public void displayStudent(){
		System.out.println(sno + " : " + sname + " : " + age);
	}
	
}
