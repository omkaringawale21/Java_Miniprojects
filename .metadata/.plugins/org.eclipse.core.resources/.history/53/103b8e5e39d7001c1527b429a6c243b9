import javax.swing.border.EmptyBorder;

public class Calculator extends JFrame {

	JPanel panel;
	
	Calculator(){
		panel = new JPanel();
		panel.setBorder(new EmptyBorder(null));
		
		panel.setSize(500, 400);
	}
	
}
