package proState;

public class SortingArray {

	public static void main(String[] args) {
		
		int arr[] = new int[] {3, 5, 8, 1, 4, 87, 51, 89, 21, 50};
		
		for(int i = 0; i < arr.length; i++){
			for(int j = i+1; j < arr.length-1; j++){
				int temp = 0;
				if(arr[i] > arr[j]){
					temp = (int) arr[i];
					arr[j] = arr[i];
					arr[i] = temp;
				}
			}
		}
		
		for(int i = 0; i < arr.length; i++){
			System.out.println("Array Sorting : " + arr[i]);
		}
		
	}
}
