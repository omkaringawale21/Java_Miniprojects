package proState;

public class ReverseString2 {

	public static void main(String[] args) {

		String str = "Hello Omkar";
		
		String[] strSplit = str.split(" ");
		
		int l, r = strSplit.length - 1;
		
		for(l=0; l < r; l++, r--){
			String temp = strSplit[l];
			strSplit[l] = strSplit[r];
			strSplit[r] = temp;
		}
		
		for(char s : strSplit){
			System.out.print(c);
		}
		
	}

}
