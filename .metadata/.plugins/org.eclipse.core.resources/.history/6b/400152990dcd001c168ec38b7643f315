package proState;

/*
 * Problem Statement : 
 * @Author Omkar Ingawale
 */

import java.util.Scanner;

public class CircularPrime {

	public static void main(String[] args) {
		
		boolean flag = true;
		int n, num, r, c = 0;
		System.out.print("Enter a number : ");
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		num = n;
		while(num > 0){
			r = num % 10;
			c++;
			num = num / 10;
		}
		num = n;
		for(int i = 1; i <= c; i++){
			r = num % 10;
			num = num / 10;
			num = (r * (int)Math.pow(10, c - 1)) + num;
		}
		
	}

}
